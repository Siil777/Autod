@model Autod.Models.LandingPage.LandingPageDetailsViewModel

<div>
    <div class="neon">
        <div class="red">Details</div>
    </div>
    <hr />
    <div class="formHeight">
        <div class="dark-transparent-background ">
            <dl class="row">
                <div class="form-group row">
                    <label asp-for="Id" class="col-sm-2 col-form-label"></label>
                    @Html.DisplayFor(model => Model.Id)
                </div>
                <div class="form-group row">
                    <label asp-for="FirstName" class="col-sm-2 col-form-label"></label>
                    @Html.DisplayFor(model => Model.FirstName)
                </div>
                <div class="form-group row">
                    <label asp-for="LastName" class="col-sm-2 col-form-label"></label>
                    @Html.DisplayFor(model => Model.LastName)

                </div>
                <div class="form-group row">
                    <label asp-for="Email" class="col-sm-2 col-form-label"></label>
                    @Html.DisplayFor(model => Model.Email)

                </div>
                <div class="form-group row">
                    <label asp-for="CreatedAt" class="col-sm-2 col-form-label"></label>
                    @Html.DisplayFor(model => Model.CreatedAt)

                </div>
                <div class="form-group row">
                    <label asp-for="Modifieted" class="col-sm-2 col-form-label"></label>
                    @Html.DisplayFor(model => Model.Modifieted)
                </div>

            </dl>
        </div>
    </div>

    <a asp-action="index" class="btn btn-outline-primary">Back</a>

</div>
